<section class="articles">

    <!-- <h2 class="articles__heading">Songs</h2>
    <a href="/song-request" class="button">Request a Song</a> -->
    <div class="admin-title">
        <h2>Songs</h2>
        <a href="/song-request" class="button">Request a Song</a>
    </div>
    <ul class="article-ul">

        <% data.forEach(post=> { %>
            <li>
                <a href="/post/<%= post._id %>">
                    <span><%= post.title %></span>
                    <span class="article-list__date"><%= post.updatedAt.toDateString() %></span>
                </a>
            </li>
        <% }) %>

    </ul>

<% if (nextPage !== null) { %>
    <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Posts</a>
<% } %>

</section>